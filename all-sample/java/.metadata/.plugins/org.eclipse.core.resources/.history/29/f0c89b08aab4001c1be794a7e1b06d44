package javatest;

public class PostCodeChecker {

	public static String GetPostCodeFromList(String[] list) {
		var result = "";

		for (int i = 0; i < list.length; i++) {
			if (list[i].matches("\\d{7}")) {
				result = "[" + (list[i] + 1) + "]”Ô–Ú‚Ì•¶Žš—ñ[" + list[i] + "]‚Í—X•Ö”Ô†‚Å‚ ‚éB";
			}
			;
		}

		result = result.isEmpty() ? "—X•Ö”Ô†‚ªŒ©‚Â‚©‚è‚Ü‚¹‚ñB" : result;

		return result;
	}

}
